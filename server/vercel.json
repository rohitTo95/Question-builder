{
  "version": 2,
  "name": "form-builder-api",
  "builds": [
    {
      "src": "server/server.ts",
      "use": "@vercel/node",
      "config": {
        "includeFiles": "server/**"
      }
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "server/server.ts"
    },
    {
      "src": "/(.*)",
      "dest": "server/server.ts"
    }
  ],
  "env": {
    "NODE_ENV": "production"
  },
  "functions": {
    "server/server.ts": {
      "maxDuration": 10
    }
  }
}
